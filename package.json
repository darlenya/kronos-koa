{
  "name": "kronos-koa",
  "version": "0.0.0-semantic-release",
  "description": "Koa modifications needed by Kronos",
  "main": "dist/application_kronos.js",
  "module": "src/application_kronos.js",
  "scripts": {
    "cover": "nyc npm test",
    "test": "mocha tests/*-test.js",
    "semantic-release": "semantic-release",
    "precover": "rollup -c",
    "pretest": "rollup -c",
    "posttest": "markdown-doctest",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js >README.md",
    "prepare": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kronos-Integration/kronos-koa.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-koa/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-koa#readme",
  "dependencies": {
    "koa": "^2.4.1",
    "koa-compose": "^4.0.0",
    "koa-is-json": "^1.0.0",
    "on-finished": "^2.3.0",
    "statuses": "^1.3.1"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "1.0.0",
    "jsdoc-to-markdown": "^3.0.1",
    "koa-route": "^3.2.0",
    "markdown-doctest": "^0.9.1",
    "mocha": "^4.0.1",
    "nyc": "^11.3.0",
    "rollup": "^0.51.2",
    "semantic-release": "^9.1.0"
  },
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=8.9.1"
  },
  "keywords": [],
  "nyc": {
    "include": [
      "dist/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./coverage"
  },
  "template": {
    "repository": {
      "url": "https://github.com/Kronos-Tools/npm-package-template.git"
    }
  },
  "xo": {
    "space": true
  }
}

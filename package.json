{
  "name": "kronos-koa",
  "version": "0.0.0-semantic-release",
  "description": "Koa modifications needed by Kronos",
  "main": "src/application_kronos.js",
  "module": "dist/application_kronos.js",
  "scripts": {
    "cover": "nyc npm test",
    "test": "mocha tests/*_test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "precover": "npm run prepublish",
    "pretest": "npm run prepublish",
    "posttest": "markdown-doctest",
    "prepublish": "rollup --output=dist/application_kronos.js -c -- src/application_kronos.js",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js >README.md"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/kronos-koa.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-koa/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-koa#readme",
  "dependencies": {
    "koa": "^2.1.0",
    "koa-compose": "^3.2.1",
    "koa-is-json": "^1.0.0",
    "on-finished": "^2.3.0",
    "statuses": "^1.3.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-http": "^1.0.0",
    "cz-conventional-changelog": "^2.0.0",
    "koa-route": "^3.2.0",
    "mocha": "^3.2.0",
    "semantic-release": "^6.3.6",
    "jsdoc-to-markdown": "^3.0.0",
    "markdown-doctest": "^0.9.1",
    "nyc": "^10.1.2",
    "rollup": "^0.41.5"
  },
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=7.6.0"
  },
  "keywords": [],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "nyc": {
    "include": [
      "dist/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./coverage"
  }
}
